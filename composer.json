{
  "name": "core/php-pure",
  "type": "project",
  "description": "The PHP core pure",
  "keywords": [
    "php"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [],
    "files": [
      "config/helper.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "altorouter/altorouter": "^1.2",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3"
  },
  "require-dev": {
    "symfony/var-dumper": "^4.3"
  },
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.default', '.env');\""
    ],
    "post-create-project-cmd": []
  }
}
